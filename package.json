{
  "name": "quick-explorer",
  "displayName": "Quick Explorer",
  "description": "Quick and interactive explorer for VSCode",
  "version": "0.0.3",
  "publisher": "nacn",
  "repository": {
    "type": "git",
    "url": "https://github.com/NaokiIshimura/vscode-quick-explorer.git"
  },
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "folder",
    "viewer",
    "explorer",
    "navigation"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "quickExplorer",
          "name": "Quick Explorer"
        }
      ]
    },
    "commands": [
      {
        "command": "quickExplorer.changeDirectory",
        "title": "Change Directory"
      },
      {
        "command": "quickExplorer.refresh",
        "title": "Refresh Quick Explorer"
      },
      {
        "command": "quickExplorer.goUp",
        "title": "Go to Parent Directory"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "lint": "eslint src --ext ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.8.1",
    "@types/vscode": "^1.105.0",
    "@vscode/vsce": "^3.6.2",
    "typescript": "^5.9.3"
  }
}